<html>
  <head>
    <style type="text/css">
      html,
      body {
        background: black;
        margin: 0px;
        padding: 0px;
        border-style: none;
      }
      .img {
        width: 100%;
        height: 100%;
      }
    </style>
  </head>

  <body scroll="no">
    <% if (server == 1) { %>
    <img src="/<%= season %>/Perk1.png" class="img" border="0" />
    <% } else if (server == 2) { %>
    <img src="/<%= season %>/Perk2.png" class="img" border="0" />
    <% } else if (server == 3) { %>
    <img src="/<%= season %>/Perk3.png" class="img" border="0" />
    <% } else if (server == 4) { %>
    <img src="/<%= season %>/VS.png" class="img" border="0" />
    <% } else if (server == 5) { %>
    <img src="/<%= season %>/Realism.png" class="img" border="0" />
    <% } else if (server == 6) { %>
    <img src="/<%= season %>/hard.png" class="img" border="0" />
    <% } else if (server == 7) { %>
    <img src="/<%= season %>/elite.png" class="img" border="0" />
    <% } else if (server == 8) { %>
    <img src="/<%= season %>/maps.png" class="img" border="0" />
    <% } else if (server == 9) { %>
    <img src="/<%= season %>/sky.png" class="img" border="0" />
    <% } else if (server == 10) { %>
    <img src="/<%= season %>/mutant.png" class="img" border="0" />
    <% } else if (server == 11) { %>
      <img src="/<%= season %>/cup.png" class="img" border="0" />
    <% } else if (server == 12) { %>
      <img src="/<%= season %>/weekend.png" class="img" border="0" />
    <% } else if (server == 13) { %>
      <img src="/<%= season %>/Rework.png" class="img" border="0" />
    <% } else if (server == 20) { %>
    <img src="/<%= season %>/EU.png" class="img" border="0" />
    <% } else if (server == 21) { %>
    <img src="/<%= season %>/EU.png" class="img" border="0" />

    <% } else { %>
    <img src="/<%= season %>/else.png" class="img" border="0" />
    <% } %>
  </body>
</html>
