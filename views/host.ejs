<html>
  <head>
    <style type="text/css">
      html,
      body {
        background: black;
        margin: 0px;
        padding: 0px;
        border-style: none;
      }
      .img {
        width: 100%;
        height: 100%;
      }
    </style>
  </head>

  <body scroll="no">
    <% const serverImages = {
      1: 'Perk1.png',
      2: 'Perk2.png',
      3: 'Perk3.png',
      4: 'VS.png',
      5: 'Realism.png',
      6: 'hard.png',
      7: 'elite.png',
      8: 'maps.png',
      9: 'sky.png',
      10: 'mutant.png',
      11: 'cup.png',
      12: 'weekend.png',
      13: 'Rework.png',
      20: 'EU.png',
      21: 'EU.png'
    }; %>

    <% if (serverImages[server]) { %>
      <img src="/<%= season %>/<%= serverImages[server] %>" class="img" border="0" />
    <% } else { %>
      <img src="/<%= season %>/else.png" class="img" border="0" />
    <% } %>
  </body>
</html>
